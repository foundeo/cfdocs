	"examples": [
		{
			"title": "Script Syntax",
			"description": "",
			"code": "count = 10; \r\n if (count > 20) { \r\n writeOutput(count); \r\n } else if (count == 8) { \r\n writeOutput(count); \r\n } else { \r\n writeOutput(count); \r\n }",
