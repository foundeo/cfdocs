	"links": [],
	"examples": [
        {
            "title": "Simple Example",
            "description": "Deletes 2nd item from the list",
            "code": "listDeleteAt(\"foo,bar,lorem,ipsum\", 2)",
            "result": "foo,lorem,ipsum",
            "runnable":true
        },
        {
