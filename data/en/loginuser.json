{
    "engines": {"lucee": {"docs": "https://docs.lucee.org/reference/functions/loginuser.html"}},
    "name": "loginuser",
    "description": "Identifies an authenticated user to ColdFusion. Specifies the user ID and roles. Used within a cflogin tag.",
    "syntax": "<loginuser name=\"\", password=\"\", roles=\"\"/>",
    "returns": "void",
    "type": "tag",
    "params": [
        {
            "name": "name",
            "description": "The Name of the User",
            "default": false,
            "type": "string",
            "required": true
        },
        {
            "name": "password",
            "description": "The Password of the User",
            "default": false,
            "type": "string",
            "required": true
        },
        {
            "name": "roles",
            "description": "The Roles of the user in comma separated form.\n",
            "default": false,
            "type": "any",
            "required": true
        }
    ]
}