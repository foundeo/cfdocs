	"description":"Gets the mathematical constant p, accurate to 15 digits\n, but only shows 11 when simply outputted.",
	"params": [],
	"engines": {
		"coldfusion": {"minimum_version":"", "notes":"", "docs":"https://helpx.adobe.com/coldfusion/cfml-reference/coldfusion-functions/functions-m-r/pi.html"},
		"lucee": {"minimum_version":"", "notes":"", "docs":"https://docs.lucee.org/reference/functions/pi.html"},
		"railo": {"minimum_version":"", "notes":"", "docs":"http://railodocs.org/index.cfm/function/pi"},
		"openbd": {"minimum_version":"", "notes":"", "docs":"http://openbd.org/manual/?/function/pi"}
	},
	"links": [],
	"examples": [
		{
			"title":"pi to 11 digits",
			"description":"",
			"code":"pi()",
			"result":"3.14159265359",
			"runnable":true
		},{
			"title":"Display all 15 digits with numberFormat",
			"description":"",
			"code":"numberFormat(pi(),\"0.000000000000000\")",
			"result":"3.141592653589793",
			"runnable":true	
		},{
			"title":"ColdFusion only shows up to 15 digits of pi",
			"description":"After 15th digit CF outputs 0's",
			"code":"numberFormat(pi(),\"0.0000000000000000000\")",
			"result":"3.1415926535897930000",
			"runnable":true	
		}
	]
}
