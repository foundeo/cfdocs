<cfset data = {related=[], name="Missing Functions", type="listing"}>
<cfloop list="#funcList#" index="f">
    <cfif NOT ArrayFindNoCase(application.index.functions, f)>
        <cfset ArrayAppend(data.related, f)>
    </cfif>
</cfloop>
<cfset percentLeft = NumberFormat ( (arrayLen(data.related)/listLen(funcList)) * 100 , "_.__")>
<cfif arrayLen(data.related) EQ 0>
	<cfset data.description = "We have all #listLen(funcList)# functions documented as of CF11. Look for missing examples now.">
<cfelse>
	<cfset data.description = "We have docs for #arrayLen(application.index.functions)# functions (#listLen(funcList)# in CF11), but we still need to add documentation for the following #arrayLen(data.related)# functions (#percentLeft#%).">
</cfif>
